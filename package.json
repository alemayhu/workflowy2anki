{
  "name": "workflowy2anki",
  "description": "Go from WorkFlowy outlines to Anki Cards easily!",
  "keywords": [
    "WorkFlowy",
    "anki",
    "workflowy2anki",
    "imba",
    "electron"
  ],
  "author": "Alexander Alemayhu",
  "version": "0.0.2",
  "engines": {
    "node": ">=6.0.0"
  },
  "main": "main.js",
  "scripts": {
    "test": "imba test/create_deck.imba",
    "build": "webpack --mode=production",
    "dev": "webpack-dev-server --mode=development --content-base dist/",
    "watch": "webpack --watch --progress --colors --display-modules"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/alemayhu/workflowy2anki.git"
  },
  "license": "MIT",
  "dependencies": {
    "anki-apkg-export": "^4.0.3",
    "cheerio": "^1.0.0-rc.3",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-saver": "^2.0.2",
    "imba": "^2.0.0-alpha.54",
    "jszip": "^3.3.0",
    "postcss-loader": "^3.0.0",
    "script-loader": "^0.7.2",
    "style-loader": "^1.2.1",
    "tailwindcss": "^1.4.6"
  },
  "devDependencies": {
    "webpack": "^4.43.0",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.7.1"
  },
  "build": {
    "appId": "com.alemayhu.workflowy2anki",
    "mac": {
      "category": "public.app-category.utilities"
    },
    "files": [
      "main.js",
      "dist/app.js",
      "dist/index.html",
      "build",
      "node_modules"
    ]
  }
}
